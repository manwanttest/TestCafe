IMAGE=cafeimage
TEST=runner filename=Modules/GitHubTests.js testname=Test.*
FILTERTEST=runner filename=Modules/*.js env=QA testname=Test.DD.*
FILTERFIXTURE=runner filename=Modules/*.js env=QA fixturename=Fixture.POM
FILTERTESTMETA=runner filename=Modules/*.js env=QA testmeta=Env,QA
FILTERFIXTUREMETA=runner filename=Modules/*.js env=QA fixturemeta=Test,Regression
PARALLELFIXTURES=parallel --filename=ParallelTests/Concurrent.js  Modules/GitHubTests.js,fixture=Fixture.POM  Modules/DataDrivenTests.js,testmeta=Env:QA  Modules/TestCafeFeatures.js,test=Using*
FLAGS= --privileged --no-sandbox --disable-gpu --disable-dev-shm-usage